{
  "name": "History Watcher",
  "manifest_version": 2,
  "version": "0.1",

  "description": "Takes you back to the website you were on at this time, one week ago",

  "permissions": [
    "tabs",
    "<all_urls>",
    "history"
  ],

  "browser_action": {
    "default_title": "Blast from the past"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  }
}
